{
    "name": "nuril91/chuck-norris-jokes",
    "description": "Create random Chuck Norris Jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nur iLham",
            "email": "nur.ilhamyuda@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Nuril91\\ChuckNorrisJokes\\": "src/"
        }
    },
    "autoload-dev": {
